{
  "chalk": {
    "match": ["chalk.", "chalk["],
    "import": "import chalk from 'chalk';"
  },
  "inquirer": {
    "match": ["inquirer.prompt", "inquirer."],
    "import": "import inquirer from 'inquirer';"
  },
  "Board": {
    "match": ["<Board", "BoardItem"],
    "import": "import { Board, BoardItem } from '@/components/Board';"
  },
  "useAppSelector": {
    "match": ["useAppSelector"],
    "import": "import { useAppSelector } from '@/store/hooks';"
  }
}